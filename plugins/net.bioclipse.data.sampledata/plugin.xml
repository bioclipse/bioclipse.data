<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="net.bioclipse.data.install" name="installdata" schema="schema/net.bioclipse.data.install.exsd"/>

   <extension
      point = "org.eclipse.ui.newWizards">
	<category
	   id = "net.bioclipse.data"
	   name="Data">
	</category>
 	<wizard
	   id = "net.bioclipse.data.sampledata.wizards.NewSampleDataProjectWizard"
	   name = "New Sample Data project"
	   project="true"
	   class="net.bioclipse.data.sampledata.wizards.NewSampleDataProjectWizard"
	   category="net.bioclipse.data"
	   icon="icons/sample.gif">
   	   <description>New QSAR Wizard</description>
	   <selection class="org.eclipse.core.resources.IResource"/>
	</wizard>
</extension>

<extension
      point="net.bioclipse.data.install">
      <folder 
      	name="2D structures"
      	description="Sample structures with 2D coordinates"
      	location="data/2DStructures"
      />
      <folder 
      	name="3D Structures"
      	description="Sample structures with 3D coordinates"
      	location="data/3DStructures"
      />
      <folder
      	name="Amino acids"
      	description="3D structures for some amino acids"
      	location="data/AminoAcids"
      />
      <folder 
      	name="JavaScripts"
      	description="Sample scripts in Javascript"
      	location="data/JavaScript"
      />
      <folder 
      	name="Jmol Scripts"
      	description="Sample scripts in Javascript"
      	location="data/jmolScripts"
      />
      <folder 
      	name="misc"
      	description="Some misc sample files"
      	location="data/misc"
      />
      <folder 
      	name="Nucleobases"
      	description="Some sample nucleobases"
      	location="data/nucleobases"
      />
      <folder 
      	name="PDB"
      	description="Sample files in PDB format"
      	location="data/pdb"
      />
      <folder 
      	name="reactions"
      	description="Some chemical reactions"
      	location="data/reactions"
      />
      <folder 
      	name="sequences"
      	description="Sample sequences"
      	location="data/sequences"
      />
      <folder 
      	name="spectra"
      	description="Some spectra files"
      	location="data/spectra"
      />
</extension>

</plugin>
